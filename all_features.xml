<?xml version="1.0"?>

<features_calculation>
    <windowing window="2500" step="1250" />

    <emg_desc>
        <feature name="IAV" /> <!--Integral Absolute Value-->
        <feature name="AAC" /> <!--Average Amplitude Change-->
        <feature name="ApEn" m="2" r="0.2"/> <!--Approximate Entropy-->
        <feature name="AR" order="4" /> <!--Auto-Regressive Coefficients-->
        <feature name="CC" order="4" /> <!--Cepstral Coefficients-->
        <feature name="DASDV" /> <!--Difference Absolute Standard Deviation Value-->
        <feature name="Kurt" /> <!--Kurtosis-->
        <feature name="LOG" /> <!--LOG Detector-->
        <feature name="MAV1" /> <!--Modified Mean Absolute Value Type 1-->
        <feature name="MAV2" /> <!--Modified Mean Absolute Value Type 2-->
        <feature name="MAV" /> <!--Mean Absolute Value-->
        <feature name="MAVSLP" /> <!--Mean Absolute Value Slope-->
        <feature name="MHW" /> <!--Multiple Hamming Windows-->
        <feature name="MTW" windowslope="0.25"/> <!--Multiple Trapezoidal Windows-->
        <feature name="MYOP" threshold="100"/> <!--Myopulse Percentage Rate-->
        <feature name="RMS" /> <!--Root Mean Square-->
        <feature name="SampleEn" m="2" r="0.2"/> <!--Sample Entropy-->
        <feature name="Skew" /> <!--Skewness-->
        <feature name="SSC" threshold="16"/> <!--Slope Sign Change-->
        <feature name="SSI" /> <!--Simple Square Integral-->
        <feature name="TM" order="4" /> <!--Absolute Temporal Moment-->
        <feature name="VAR"  /> <!--Variance-->
        <feature name="V" v="3"/> <!--V-Order-->
        <feature name="WAMP" threshold="10"/> <!--Willison Amplitude-->
        <feature name="WL" /> <!--Waveform Length-->
        <feature name="ZC" threshold="30" /> <!--Zero Crossing-->
        <feature name="MNF" /> <!--Mean Frequency-->
        <feature name="MDF" /> <!--Median Frequency-->
        <feature name="PKF" /> <!--Peak Frequency-->
        <feature name="MNP" /> <!--Mean Power-->
        <feature name="TTP" /> <!--Total Power-->
        <feature name="FR" flb="[15, 45]" fhb="[90, 500]" /> <!--Frequency Ratio-->
        <feature name="VCF" /> <!--Variance of Central Frequency-->
        <feature name="PSR" n="20" /> <!--Power Spectrum Ratio-->
        <feature name="SNR" powerband="[10, 1000]" noiseband="[500, 1000]" /> <!--Signal-to-Noise Ratio-->
        <feature name="DPR" band="[35, 600]" n="13" /> <!--Maximum-to-minimum Drop in Power Density Ratio-->
        <feature name="OHM" /> <!--Power Spectrum Deformation-->
        <feature name="MAX" order="6" cutoff="5" /> <!--Maximum Amplitude-->
        <feature name="SMR" n="13" /> <!--Signal-to-Motion Artifact Ratio-->
        <feature name="BC" y_box_size_multiplier="3" subsampling="25" /> <!--Box-Counting Dimension-->
        <feature name="PSDFD" power_box_size_multiplier="0.1" subsampling="50" /> <!--Power Spectral Density Fractal Dimension-->
    </emg_desc>
</features_calculation>